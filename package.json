{
  "name": "@thayl/typescript-boilerplate",
  "version": "1.0.0",
  "license": "GPL-3.0",
  "author": "Gabriel Côté",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "test": "jest --config jest.config.ts --runInBand --no-cache",
    "build:dev": "yarn clean && tsc-watch --compiler ttypescript/bin/tsc --onSuccess \"node --enable-source-maps ./dist/src/index.js\"",
    "build": "yarn clean && node_modules/ttypescript/bin/tsc",
    "clean": "rimraf dist",
    "start": "node --enable-source-maps ./dist/src/index.js",
    "generate:env": "node ./generate-local-config.js",
    "lint:fix": "eslint './src/**/*.{ts,tsx}' --quiet --fix",
    "db:migrate": "yarn ts-node --transpile-only ./node_modules/typeorm/cli.js  migration:run -d ./src/migrations/datasource.js"
  },
  "peerDependencies": {},
  "devDependencies": {
    "@types/jest": "^28.1.1",
    "@types/lodash": "^4.14.155",
    "@types/node": "^18.0.0",
    "@types/uuid": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "bufferutil": "^4.0.1",
    "canvas": "^2.5.0",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^28.1.1",
    "mockttp": "^3.1.0",
    "moq.ts": "^9.0.2",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.0",
    "ts-jest": "^28.0.5",
    "ts-node": "^10.8.1",
    "tsc-watch": "^5.0.3",
    "ttypescript": "^1.5.11",
    "typescript": "^4.7.4",
    "typescript-transform-paths": "^3.3.1",
    "utf-8-validate": "^5.0.2"
  },
  "dependencies": {
    "@fastify/type-provider-typebox": "^1.0.0",
    "@sinclair/typebox": "^0.23.5",
    "axios": "^0.27.2",
    "dotenv": "^16.0.1",
    "fastify": "^4.0.2",
    "inversify": "^6.0.1",
    "lodash": "^4.17.21",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.6"
  }
}
